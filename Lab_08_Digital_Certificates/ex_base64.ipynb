    "<h1>Base64</h1>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import base64\n",
    "\n",
    "string = \"Hello World\"\n",
    "\n",
    "string_bytes = string.encode('utf-8')\n",
    "\n",
    "base64_bytes = base64.b64encode(string_bytes)\n",
    "\n",
    "base64_string = base64_bytes.decode('utf-8')\n",
    "print(base64_string)\n",
    "\n",
    "s = base64.b64decode(base64_string).decode('utf-8')\n",
    "print(s)"
   ]
  }
